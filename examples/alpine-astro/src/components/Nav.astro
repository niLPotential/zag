---
import { dataAttr } from "@zag-js/dom-query"
import { routesData } from "@zag-js/shared"
---

<aside class="nav">
  <header>Zagjs</header>
  {
    routesData
      .sort((a, b) => a.label.localeCompare(b.label))
      .map((route) => (
        <a data-active={dataAttr(Astro.url.pathname === route.path)} href={route.path}>
          {route.label}
        </a>
      ))
  }
</aside>
